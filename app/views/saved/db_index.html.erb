<%# for database display purposes: compare%>
<% unless logged_in? %>
    <p>You aren't logged in, log in first. </p>
<% end %>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @saved_items.each do |s| %>
      <tr>
        <td><%= s.item.name %></td>
        <td><%= s.user.id %></td>
      </tr>
    <% end %>
  </tbody>
</table>
